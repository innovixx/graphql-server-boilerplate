{
  "compilerOptions": {
    "target": "ES6",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "rootDir": "./src",
    "outDir": "./dist",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "strict": true,
  },
  "include": [
    "src",
    "*.d.ts",
    "eslint.config.mjs",
  ],
  "exclude": [
    "node_modules",
    "databases"
  ],
}